  position: absolute;\n    width: 100%;\n    height: 100%;\n    top: 0;\n    left: 0;\n\n    .center {\n      position: absolute;\n      top: 130px;\n      left: 50%;\n      transform: translateX(-50%);\n      display: inline-block;\n      height: 100%;\n    }\n\n    .title {\n      background-color: #848484;\n      position: absolute;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 90px;\n      line-height: 90px;\n      padding-left: 40px;\n      padding-top: 10px;\n      border-radius: 25px 25px 0 0;\n      color: #fff;\n      font-size: 40px;\n    }\n  }\n\n  // Calculator\n  .calculator {\n\n    .center {\n      width: 430px;\n    }\n\n    #result,\n    #operator,\n    button {\n      height: 50px;\n      margin: 5px;\n      border: none;\n    }\n\n    #result,\n    #operator {\n      border: 2px solid #ccc;\n      margin-bottom: 15px;\n      font-size: 2rem;\n    }\n\n    #result {\n      width: 295px;\n      height: 90px;\n      text-align: right;\n      padding-right: 10px;\n    }\n\n    #operator {\n      width: 90px;\n      height: 90px;\n      text-align: center;\n    }\n\n    button {\n      width: 90px;\n      height: 90px;\n      cursor: pointer;\n      font-size: 1.5rem;\n      background-color: #eee;\n\n      &:active {\n        background-color: #ddd;\n      }\n    }\n  }\n\n}\n\n// Sub-Function\n.sub-function {\n  width: 46%;\n  position: relative;\n\n  .login {\n    position: absolute;\n    width: 130%;\n    top: 0;\n    right: 0;\n    display: flex;\n    align-items: center;\n    height: 180px;\n    margin-bottom: 30px;\n    border-radius: 25px;\n    background-color: #fff;\n    @include box-shadow;\n\n    .flex {\n      display: flex;\n      justify-content: center;\n    }\n\n    #login-form,\n    #user {\n      position: absolute;\n      width: 100%;\n      transform: translateX(65px);\n    }\n\n    #login-form {\n\n      input {\n        outline: none;\n      }\n\n      input[type=\"text\"] {\n        border: none;\n        border-bottom: 2px solid #333;\n        text-align: center;\n        width: 50%;\n        color: #333;\n        font-size: 3rem;\n        padding-top: 15px;\n        padding-bottom: 10px;\n\n        &::placeholder {\n          opacity: 1;\n          color: #ddd;\n          font-size: 2rem;\n          transition: .2s;\n        }\n\n        &:focus::placeholder {\n          visibility: hidden;\n          opacity: 0;\n        }\n      }\n\n      input[type=\"submit\"] {\n        height: 80%;\n        margin-top: 20px;\n        font-size: 1.3rem;\n        border: none;\n        background-color: #bbb;\n        padding: 15px 30px;\n        border-radius: 25px;\n        margin-left: 20px;\n        color: #fff;\n        transition: 0.2s;\n        cursor: pointer;\n\n        &:hover {\n          color: #333;\n          background-color: #eee;\n        }\n      }\n    }\n\n    #user {\n\n      #greeting {\n        font-size: 1.7rem;\n        text-align: center;\n        line-height: 1.5em;\n        color: #bbb;\n        margin-bottom: 10px;\n      }\n\n      #user-name {\n        font-size: 3.5rem;\n      }\n\n      #edit {\n        border: none;\n        width: 30px;\n        background-color: transparent;\n        color: rgb(165, 165, 165);\n        transform: scale(1.7) translateY(8px);\n        cursor: pointer;\n        margin-left: 13px;\n        margin-top: 7px;\n        height: 100%;\n\n        &:hover {\n          color: #ddd;\n        }\n      }\n    }\n  }\n\n  // CLOCK\n  #clock {\n    height: 180px;\n    background-color: #ec904e;\n    color: #fff;\n    text-align: center;\n    font-size: 5.5rem;\n    line-height: 2em;\n    border-radius: 25px;\n    margin-top: 210px;\n    margin-bottom: 30px;\n    font-family: \"Open Sans\", sans-serif;\n  }\n\n  .wrap {\n    width: 100%;\n    height: 180px;\n    position: relative;\n  }\n\n  // QUOTE\n  #quote {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 65%;\n    height: 100%;\n    padding: 25px;\n    overflow: hidden;\n    border-radius: 25px;\n    font-size: 1.1rem;\n    transition: 0.3s;\n    left: 0;\n    bottom: -140px;\n    transition: opacity 0.3s;\n    background-color: #fff;\n\n    &::after {\n      position: absolute;\n      content: 'Motivation';\n      color: #fff;\n      font-size: 2rem;\n      text-align: center;\n      line-height: 180px;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      background-color: #477DA9;\n      transition: .5s;\n    }\n\n    &:hover::after {\n      top: -180px;\n    }\n\n    .quote {\n      font-weight: 600;\n      line-height: 1.5em;\n    }\n\n    .author {\n      position: absolute;\n      bottom: 25px;\n      left: 25px;\n      display: block;\n      margin-top: 15px;\n      font-size: 0.9rem;\n      color: rgb(103, 103, 103);\n    }\n  }\n\n  // WEATHER\n  #weather {\n    position: absolute;\n    top: 0;\n    right: 0;\n    color: rgb(253, 242, 242);\n    display: inline-block;\n    width: 30%;\n    height: 100%;\n    font-size: 1.5rem;\n    text-shadow: 0 0 5px #333;\n    transition: 0.3s;\n    background-color: #848484;\n    padding: 15px;\n    padding-left: 25px;\n    border-radius: 20px;\n\n    span {\n      display: block;\n      line-height: 1.3;\n    }\n\n    .city {\n      margin-top: 10px;\n    }\n\n    .temperature {\n      font-size: 3rem;\n    }\n\n    .weather {\n      display: inline-block;\n    }\n\n    .cloud {\n      margin-right: 10px;\n      transform: translateY(5px);\n    }\n  }\n\n  .btns {\n    position: absolute;\n    bottom: 0;\n\n    button {\n      position: relative;\n      border: none;\n      width: 110px;\n      height: 110px;\n      border-radius: 50%;\n      margin-right: 15px;\n      transition: .2s;\n      cursor: pointer;\n      background-color: #fff;\n      @include box-shadow;\n\n      &:hover {\n        transform: translateY(-5px);\n      }\n\n      &:active {\n        transform: translateY(3px);\n      }\n    }\n\n    img {\n      transform: scale(17%);\n      position: absolute;\n    }\n\n    #btn-todo {\n      img {\n        top: -130px;\n        left: -130px;\n      }\n    }\n\n    #btn-paint {\n      img {\n        top: -130px;\n        left: -130px;\n      }\n    }\n\n    #btn-calculate {\n      img {\n        top: -130px;\n        left: -103px;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"474636eae2a8299547d582cc2f8ea623","cacheData":{"env":{}}}